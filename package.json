{
  "name": "personal-website",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "dev": "next dev",
    "watch": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "deploy": "yarn build && yarn export",
    "format": "prettier --write .",
    "lint:js": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:css": "npx stylelint '**/*.scss'",
    "lint": "yarn run lint:js && yarn run lint:css",
    "test": "jest",
    "start:cms": "npx netlify-cms-proxy-server",
    "make-flexible": "node node_modules/@martijnnieuwenhuizen/generate-component ./components/flexibles $1 --with-netlify-cms",
    "make-module": "node node_modules/@martijnnieuwenhuizen/generate-component ./components/modules $1",
    "make-partial": "node node_modules/@martijnnieuwenhuizen/generate-component ./components/partials $1"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "^12.1.0",
    "next-seo": "^4.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga4": "^1.0.6",
    "react-intersection-observer": "^8.31.0",
    "react-markdown": "^6.0.1"
  },
  "devDependencies": {
    "@martijnnieuwenhuizen/generate-component": "^0.6.0",
    "@netlify/plugin-nextjs": "^3.9.0",
    "@svgr/webpack": "^5.5.0",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "frontmatter-markdown-loader": "^3.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "prettier": "^2.2.1",
    "sass": "^1.32.4",
    "stylelint": "^13.7.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.1.3"
  }
}
